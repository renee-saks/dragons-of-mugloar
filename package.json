{
  "name": "dragons-of-mugloar",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "format:check": "prettier --log-level warn --ignore-path=./.gitignore --check \"**/*.{js,ts,css,scss,sh,html,md,json,yaml,yml}\"",
    "format:fix": "npm run format:check -- --write",
    "lint:check": "eslint -f mo \"**/*.{js,ts,html,json}\"",
    "lint:fix": "npm run lint:check -- --fix",
    "style:check": "stylelint \"**/*.{css,scss}\"",
    "style:fix": "npm run style:check -- --fix",
    "fix": "npm run format:fix && npm run lint:fix && npm run style:fix"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/ngrx-toolkit": "19.1.0",
    "@angular/animations": "19.2.5",
    "@angular/cdk": "19.2.8",
    "@angular/common": "19.2.5",
    "@angular/compiler": "19.2.5",
    "@angular/core": "19.2.5",
    "@angular/forms": "19.2.5",
    "@angular/material": "19.2.8",
    "@angular/platform-browser": "19.2.5",
    "@angular/platform-browser-dynamic": "19.2.5",
    "@angular/router": "19.2.5",
    "@ngrx/signals": "19.1.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.6",
    "@angular/cli": "19.2.6",
    "@angular/compiler-cli": "19.2.5",
    "@types/jasmine": "5.1.7",
    "angular-cli-ghpages": "2.0.3",
    "angular-eslint": "19.3.0",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-formatter-mo": "2.3.0",
    "eslint-plugin-jasmine": "4.2.2",
    "eslint-plugin-jsonc": "2.20.0",
    "jasmine-core": "5.6.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.5.3",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-sh": "0.16.1",
    "stylelint": "16.18.0",
    "stylelint-config-standard": "38.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1"
  }
}
